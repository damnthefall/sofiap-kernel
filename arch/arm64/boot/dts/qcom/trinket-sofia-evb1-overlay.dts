/dts-v1/;
/plugin/;

#include <dt-bindings/clock/qcom,gcc-trinket.h>
#include "trinket-sofia-common-overlay.dtsi"

/ {
	model = "sofia";
	compatible = "qcom,trinket-sofia", "qcom,trinket-moto", "qcom,trinket";
	qcom,msm-id = <394 0x0>;
	qcom,board-id = <0x46 0xA100>;
};

&et520 {
	egistec,gpio_ldo1p8_en = <&tlmm 128 0x0>;
};

&fpc {
	pinctrl-0 = <&ldo18_en_active &ldo33_en_active &int_active &rst_active>;
	pinctrl-1 = <&ldo18_en_suspend &ldo33_en_suspend &int_suspend &rst_suspend>;
};

