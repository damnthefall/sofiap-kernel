&i2c_freq_custom {
	qcom,hw-thigh = <23>;
	qcom,hw-tlow = <30>;
	qcom,hw-tsu-sto = <20>;
	qcom,hw-tsu-sta = <20>;
	qcom,hw-thd-dat = <18>;
	qcom,hw-thd-sta = <19>;
	qcom,hw-tbuf = <31>;
	qcom,hw-scl-stretch-en = <0>;
	qcom,hw-trdhld = <3>;
};

&soc {
	qcom,csiphy@1628000 {
		qcom,clock-rates = <0 0 311000000 0 0 311000000 0
			0 311000000 0 0>;
	};

	qcom,csiphy@1629000 {
		qcom,clock-rates = <0 0 311000000 0 0 311000000 0
			0 311000000 0 0>;
	};

	qcom,csiphy@162a000 {
		qcom,clock-rates = <0 0 311000000 0 0 311000000 0
			0 311000000 0 0>;

	};

	vfe0: qcom,vfe0@5c10000 {
		qcom,clock-rates = <0 0 403200000 0 0 0 0 0 0 0
					0 0 576000000 0 0 0 0 0 0 0
					0 0 576000000 0 0 0 0 0 0 0>;
	};

	vfe1: qcom,vfe1@5c14000 {
		qcom,clock-rates = <0 0 403200000 0 0 0 0 0 0 0
					0 0 576000000 0 0 0 0 0 0 0
					0 0 576000000 0 0 0 0 0 0 0>;
	};

	led_flash_rear: qcom,camera-flash@0 {
		cell-index = <0>;
		compatible = "qcom,camera-flash";
		qcom,flash-source = <&pmi632_flash0>;
		qcom,torch-source = <&pmi632_torch0>;
		qcom,switch-source = <&pmi632_switch0>;
		status = "ok";
	};
};

&qupv3_se3_i2c {
	status = "ok";
	stmvl53l0@29 {
		compatible = "st,stmvl53l0_i2c";
		reg = <0x29>;
		gpios =<&tlmm 57 0>, <&tlmm 70 0>, <&tlmm 58 0>;
		qcom,gpio-reset = <0>;
		qcom,gpio-vana = <1>;
		qcom,gpio-req-tbl-num = <0 1 2>;
		qcom,gpio-req-tbl-flags = <0 1 2>;
		qcom,gpio-req-tbl-label = "TOF_XSHUTDOWN", "TOF_VANA", "TOF_INT";
		pinctrl-names = "laser_default", "laser_suspend";
		pinctrl-0 = <&laser_active>;
		pinctrl-1 = <&laser_suspend>;
		st,sensorthreshold = <60 65>;
		st,xtalkval = <0>;
		status = "ok";
	};
};

&tlmm {
	pm8008_active: pm8008_active {
		mux {
			pins = "gpio67";
		};

		config {
			pins = "gpio67";
		};
	};

	laser_active: laser_active {
		mux {
			pins ="gpio57", "gpio70", "gpio58";
			function = "gpio";
		};

		config {
			pins ="gpio57", "gpio70", "gpio58";
			drive-strength = <2>;
				bias-pull-up; /* PULL Up */
		};
	};
	laser_suspend: laser_suspend{
		mux {
			pins ="gpio57", "gpio58";
			function = "gpio";
		};

		config {
			pins ="gpio57", "gpio58";
			drive-strength = <2>;
			bias-pull-down; /* PULL Down */
		};
	};

	cam_sensor_rear_gpio_active: cam_sensor_rear_gpio_active {
		/* AF, AVDD, DVDD */
		mux {
			pins = "gpio47", "gpio49", "gpio45";
			function = "gpio";
		};

		config {
			pins = "gpio47", "gpio49", "gpio45";
			bias-disable; /* No PULL */
			drive-strength = <2>; /* 2 MA */
		};
	};

	cam_sensor_rear_gpio_suspend: cam_sensor_rear_gpio_suspend {
		/* AF, AVDD, DVDD */
		mux {
			pins = "gpio47", "gpio49", "gpio45";
			function = "gpio";
		};

		config {
			pins = "gpio47", "gpio49", "gpio45";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
		};
	};

	cam_sensor_aux_gpio_active: cam_sensor_aux_gpio_active {
		/* AVDD, DVDD */
		mux {
			pins = "gpio132", "gpio84";
			function = "gpio";
		};

		config {
			pins = "gpio132", "gpio84";
			bias-disable; /* No PULL */
			drive-strength = <2>; /* 2 MA */
		};
	};

	cam_sensor_aux_gpio_suspend: cam_sensor_aux_gpio_suspend {
		/* AVDD, DVDD */
		mux {
			pins = "gpio132", "gpio84";
			function = "gpio";
		};

		config {
			pins = "gpio132", "gpio84";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
		};
	};

	cam_sensor_front_gpio_active: cam_sensor_front_gpio_active {
		/* AVDD, DVDD, MIPI_SEL */
		mux {
			pins = "gpio49", "gpio45", "gpio27";
			function = "gpio";
		};

		config {
			pins = "gpio49", "gpio45", "gpio27";
			bias-disable; /* No PULL */
			drive-strength = <2>; /* 2 MA */
		};
	};

	cam_sensor_front_gpio_suspend: cam_sensor_front_gpio_suspend {
		/* AVDD, DVDD, MIPI_SEL */
		mux {
			pins = "gpio49", "gpio45", "gpio27";
			function = "gpio";
		};

		config {
			pins = "gpio49", "gpio45", "gpio27";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
		};
	};

	cam_sensor_mclk3_active: cam_sensor_mclk3_active {
		/* MCLK3 */
		mux {
			pins = "gpio44";
			function = "cam_mclk";
		};

		config {
			pins = "gpio44";
			bias-disable; /* No PULL */
			drive-strength = <2>; /* 2 MA */
		};
	};

	cam_sensor_mclk3_suspend: cam_sensor_mclk3_suspend {
		/* MCLK3 */
		mux {
			pins = "gpio44";
			function = "cam_mclk";
		};

		config {
			pins = "gpio44";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
		};
	};
	cam_sensor_video_gpio_active: cam_sensor_video_gpio_active {
		/* RESET, AVDD, DVDD, MIPI_SEL */
		mux {
			pins = "gpio41", "gpio132", "gpio84", "gpio27";
			function = "gpio";
		};

		config {
			pins = "gpio41", "gpio132", "gpio84", "gpio27";
			bias-disable; /* No PULL */
			drive-strength = <2>; /* 2 MA */
		};
	};

	cam_sensor_video_gpio_suspend: cam_sensor_video_gpio_suspend {
		/* RESET, AVDD, DVDD, MIPI_SEL */
		mux {
			pins = "gpio41", "gpio132", "gpio84", "gpio27";
			function = "gpio";
		};

		config {
			pins = "gpio41", "gpio132", "gpio84", "gpio27";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
		};
	};
};

&cci {
	actuator_rear: qcom,actuator@0 {
		cell-index = <0>;
		reg = <0x0>;
		compatible = "qcom,actuator";
		qcom,cci-master = <0>;
	};

	eeprom_rear: qcom,eeprom@0 {
		cell-index = <0>;
		compatible = "qcom,eeprom";
		reg = <0x0>;
		cam_vio-supply = <&L12A>;
		qcom,cam-vreg-name = "cam_vio";
		qcom,cam-vreg-min-voltage = <1800000>;
		qcom,cam-vreg-max-voltage = <1800000>;
		qcom,cam-vreg-op-mode = <0>;
		qcom,gpio-no-mux = <0>;
		pinctrl-names = "cam_default", "cam_suspend";
		pinctrl-0 = <&cam_sensor_mclk0_active
					&cam_sensor_rear_active
					&cam_sensor_rear_gpio_active>;
		pinctrl-1 = <&cam_sensor_mclk0_suspend
					&cam_sensor_rear_suspend
					&cam_sensor_rear_gpio_suspend>;
		gpios = <&tlmm 34 0>,
				<&tlmm 48 0>,
				<&tlmm 47 0>,
				<&tlmm 49 0>,
				<&tlmm 45 0>;
		qcom,gpio-reset = <1>;
		qcom,gpio-vaf = <2>;
		qcom,gpio-vana = <3>;
		qcom,gpio-vdig = <4>;
		qcom,gpio-req-tbl-num = <0 1 2 3 4>;
		qcom,gpio-req-tbl-flags = <1 0 0 0 0>;
		qcom,gpio-req-tbl-label = "CAM_R_MCLK",
								  "CAM_R_RESET",
								  "CAM_R_AF",
								  "CAM_R_AVDD",
								  "CAM_R_DVDD";
		qcom,cci-master = <0>;
		status = "ok";
		clocks = <&clock_gcc GCC_CAMSS_MCLK0_CLK_SRC>,
				<&clock_gcc GCC_CAMSS_MCLK0_CLK>;
		clock-names = "cam_src_clk", "cam_clk";
		qcom,clock-rates = <24000000 0>;
