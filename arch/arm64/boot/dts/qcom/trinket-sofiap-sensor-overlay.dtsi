&tlmm {
    pen_int_default: pen_int_default {
			mux {
				pins = "gpio94";
				function =  "gpio";
			};
			config {
				pins = "gpio94";
				num-grp-pins = <1>;
				drive-strength = <2>;
				bias-no-pull;
			};
	};

	pen_int_sleep: pen_int_sleep {
			mux {
				pins = "gpio94";
				function =  "gpio";
			};
			config {
				pins = "gpio94";
				num-grp-pins = <1>;
				drive-strength = <2>;
				bias-no-pull;
			};
	};

	sar_int_default: sar_int_default {
			mux {
				pins = "gpio82";
				function =  "gpio";
			};
			config {
				pins = "gpio82";
				num-grp-pins = <1>;
				drive-strength = <2>;
				bias-pull-up;
			};
	};

	sar_int_sleep: sar_int_sleep {
			mux {
				pins = "gpio82";
				function =  "gpio";
			};
			config {
				pins = "gpio82";
				num-grp-pins = <1>;
				drive-strength = <2>;
				bias-pull-up;
			};
	};
};

&soc {
	pen_detect{
		compatible = "rohm,bu520xx_pen_detect";
		pinctrl-names = "default","sleep";
		pinctrl-0 = <&pen_int_default>;
		pinctrl-1 = <&pen_int_sleep>;
		rohm,nirq-gpio=<&tlmm 94 0x02>;
	};
};

/* Compatible with the external LDO or forced power

	Semtech,power-supply-type:
	<0> means the LDO from PMIC: the default value if no set "Semtech,power-supply-type";
		 The configuration for vdd-supply remains the same.
	<1> means forced power supply;
		no need set others.
	<2> means external LDO power supply;
		Need define "Semtech,eldo-gpio" for external LDO enable gpio;
*/
&qupv3_se1_i2c {
	status = "ok";
	sx933x@28 { /* SAR sensor */
		compatible = "Semtech,sx933x";
